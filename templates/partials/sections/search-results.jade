section.section#searchResultsSection
	.panel-header
		ul.nav.nav-tabs
			li.active: a(href="#resultsResidentTab", data-toggle="tab")
				span.fa.fa-street-view &nbsp;Resident
					.label.label-info {{searchResultsCount}}
			//li: a(href="#resultsOntourTab", data-toggle="tab")
				span.fa.fa-space-shuttle &nbsp;On tour (2)
			li.pull-right: a(href="#resultsRememberedTab", data-toggle="tab")
				span.fa.fa-bookmark &nbsp; Remembered
					span.label.label-info {{storage.rememberedProfiles.length}}
	.panel-body
		.tab-content
			// Tab: Residents
			include ../tabs/results-resident.jade
			// Tab: On Tour
			//include ../tabs/results-ontour.jade
			// Tab: Remembered
			include ../tabs/results-remembered.jade