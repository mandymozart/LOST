.tab-pane.fade.in.active#callForArtistsTab
	.form-group
		label.col-sm-2.control-label(for='inputType') Type
		.col-sm-10
			.radio
				label
					input#optionsType1(type='radio', name='optionsType', value='Solo Performer', checked='checked')
					| Solo Performer
			.radio
				label
					input#optionsType2(type='radio', name='optionsType', value='Band')
					| Band
			.radio
				label
					input#optionsType3(type='radio', name='optionsType', value='Orchestra')
					| Orchestra
			.radio
				label
					input#optionsType4(type='radio', name='optionsType', value='Magician')
					| Magician
	.form-group
		label.col-sm-2.control-label(for='inputGenres') Genres*
		.col-sm-10
			input.form-control(type='text',placeholder='Type to select genres',name='inputGenre',id='inputGenre')
	.form-group
		label.col-sm-2.control-label(for='inputDescription') Request notice
		.col-sm-10
			textarea.form-control(rows='3')
			.help-block Add a few lines of love to your request
	.form-group
		label.col-sm-2.control-label(for='inputProfile') Specific artist
		.col-sm-10
			input.form-control(type='text',placeholder='Contact artist',name='inputProfile',id='inputProfile')