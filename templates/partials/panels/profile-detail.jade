form.form#profileDetailForm(ng-if="isProfileSelected()")
		.alert.alert-success.alert-dismissable.fade.in(ng-if="saveStatus !== ''") 
			//button.close(type="button", data-dismiss="alert", aria-label="Close")
				span(aria-hidden="true") &times;
			&nbsp; {{saveStatus}} &nbsp;
	.col-md-5

		.form-group
		label(for="profile-name-area") Name:
		input.form-control(id="profile-name-area", rows="1",cols="50",ng-model="editprofile.name",placeholder="Profile Name")
		#subtypeSelect(ng-if="editprofile.type=='Artist'")
			.form-group
				label(for="subtype-select") Subtype:
				select(id="subtype-select", ng-model="editprofile.subtype",style="width:100%")
					option(value="{{option}}", ng-repeat="option in storage.datalists.artistTypes")
						| {{option}}
		.row
			.form-group
				label(for="genre-select") Genres:
				multiselect.input-lg(
					multiple="true", 
					ng-if="getGenreList().length > 0",
					ng-model="editgenres.data", 
					options="genre.name for genre in getIndexedList(getGenreList()) | filter:$select.search", 
					change="selected()", 
					placeholder="Genres")
		.row
			.form-group
				label(for="profile-zip") Zip Code:
				input.form-control(id="profile-zip", placeholder="ex. 0711", ng-model="editprofile.zip")
				//label(for="profile-lat") Latitude
				//input.form-control(type="number", placeholder="0",ng-model="editprofile.geolocation.lat")
				//label(for="profile-lon") Longitude
				//input.form-control(type="number", placeholder="0",ng-model="editprofile.geolocation.lon")

		.row
			.form-group
				label(for="link-input") Social Links:
				input.form-control(id="profile-link",placeholder="ex. http://www.soundcloud.com/awesome_user", ng-model="linkInput.data")
				span(ng-repeat="link in editprofile.socialLinks track by $index") {{link}} <br>
				button(ng-click="addSocialLink()") Add Link
		.row
			.form-group
				label(for="profile-about-brief-area") About:
				textarea.form-control(id="profile-about-brief-area", rows="4", cols="50", ng-model="editprofile.about")

	.col-sm-7
		leaflet(height="256px",
			width="100%",
			center="center",
			markers="markers")
		